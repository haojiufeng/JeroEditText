<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="JeroEditText">
        <!--最大字数限制-->
        <attr name="jet_maxLength" format="integer" />
        <!--最大字数的文字颜色-->
        <attr name="jet_maxTextColor" format="color" />
        <!--达到最大字数时提示的颜色-->
        <attr name="jet_fillTextColor" format="color" />
        <!--提示文本的文字大小-->
        <attr name="jet_tipTextSize" format="dimension" />
    </declare-styleable>
</resources>